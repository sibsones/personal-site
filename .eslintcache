 Archive from \"./Archive\"\nimport Freshers from './Freshers'\nimport Interactive from './Interactive'\nimport Stent from './Stent'\nimport Gems from './Gems'\nimport Botnet from './Botnet'\nimport Livecode from './Livecode'\n\n\nfunction App() {  \n  return (\n    <BrowserRouter>\n      <div className=\"App\">\n        <h1 class=\"title\">Luis Sibson</h1>\n        <Route exact path =\"/\" component={Home} />\n        <Route path =\"/portfolio\" component={Projects} />\n        <Route path = \"/archive\" component={Archive}/>\n        <Route path = \"/freshers\" component={Freshers}/>\n        <Route path = \"/interactive\" component={Interactive}/>\n        <Route path = \"/botnet\" component={Botnet}/>\n        <Route path = \"/gems\" component={Gems}/>\n        <Route path = \"/stent\" component={Stent}/>\n        <Route path = \"/livecode\" component={Livecode}/>\n\n        <div className=\"nav\">\n          <div className=\"nav-items\">\n            <Link to=\"/\" class=\"middle\">Home</Link>\n            <Link to=\"/portfolio\" class=\"middle\">Portfolio</Link>\n\n          </div>\n        </div>\n      </div>\n    </BrowserRouter>\n  );\n}\n\nexport default App;\n","/home/nosbis/Documents/personal-site/src/reportWebVitals.js",[],"/home/nosbis/Documents/personal-site/src/Projects.js",[],"/home/nosbis/Documents/personal-site/src/Home.js",["69"],"import React from \"react\"\nimport profilepic from \"./images/profilepic.jpg\"\n \nfunction Home(props) {\n  return (\n    <div class=\"home-content\">\n      <div class=\"home-desc\">\n        <img class=\"profilepic\" src={profilepic}></img>\n      </div>\n        <div class=\"about\">\n          <p>This is a portfolio of my current & previous projects and my first dive into React! <br></br> I'm a 2021 graduate in Computer Science with particular interests in client focussed Web and Mobile development.</p>\n          <a href = \"mailto: luis.sibson@gmail.com\">Contact</a>\n          <br></br>\n          <p>\n            Preferred Technologies:\n          </p>\n          <ul>\n            <li>Python</li>\n            <li>HTML | CSS | JS</li>\n            <li>Django | React</li>\n            <li>Linux | Git</li>\n            <li>Java | Rust | Haskell</li>\n            <li>MySQL | MongoDB</li>\n          </ul>\n      </div>\n    </div>\n  )\n}\n \nexport default Home;","/home/nosbis/Documents/personal-site/src/Thumbnail.js",[],["70","71"],"/home/nosbis/Documents/personal-site/src/Archive.js",["72"],"import React from \"react\"\nimport './App.css'\nimport archivepic from './images/archives.png'\n\nfunction Archive(props) {\n  return (\n    <div class=\"project-select\">\n      <h1>Subcity Radio Archive</h1>\n      <p>An interactive archival exhibition spanning 25 years.</p>\n      <a href=\"https://www.subcity.org/archive\">\n      <img src={archivepic}></img>\n      </a>\n      <p>This archive showcases 25 years of Subcity's history, team members can upload new entries through Django forms, hosting multiple filetypes, descriptions and tags. Currently ongoing redesign, as a collaboration between Web, Archives and Design teams.</p>\n      <p>Technologies: Django, Google Cloud API</p>\n    </div>\n  )\n}\n \nexport default Archive;","/home/nosbis/Documents/personal-site/src/Freshers.js",["73"],"import React from \"react\"\nimport './App.css'\nimport schedule from './images/freshers-schedule.png'\n\nfunction Freshers(props) {\n    return (\n      <div class=\"project-select\">\n        <h1>Subcity Radio Freshers Schedule 2020</h1>\n        <p>Schedule of an entire week of 24/7 remote broadcasting inline with COVID guidelines.<br></br>Current schedule displays one date at a time, we wanted to fully showcase the sheer scale of this week of broadcasting. Show descriptions fluidly pop out on hover, page is also mobile friendly :)</p>\n        <a href=\"https://www.subcity.org/freshers2020\">\n        <img src={schedule} alt=\"Archive Image\"></img>\n        </a>\n        <p>Technologies: Django</p>\n      </div>\n    )\n  }\n\nexport default Freshers","/home/nosbis/Documents/personal-site/src/Interactive.js",["74"],"import React from \"react\"\nimport './App.css'\nimport interactive from './images/interactive.png'\n\nfunction Interactive(props) {\n  return (\n    <div class=\"project-select\">\n      <h1>Subcity Radio Interactive Piece</h1>\n      <p>A shared, live interactive audio experience, adapted from <a href=\"https://github.com/lil-data/algo\">Lil Data</a>. Being interactive, multiple users control the same audio generation across different browsers.</p>\n      <a href=\"https://www.subcity.org/interactive\">\n      <img src={interactive}></img>\n      </a>\n      <p>Technologies: HTML/CSS/JS</p>\n    </div>\n  )\n}\n \nexport default Interactive;","/home/nosbis/Documents/personal-site/src/Stent.js",["75","76"],"import React from \"react\"\nimport './App.css'\nimport stentpic from './images/stentpic.jpg'\n\nfunction Stent(props) {\n  return (\n    <div class=\"project-select\">\n      <h1>Smart Stent Graph</h1>\n      <p>Third Year Group Project: Developing an Android App to display data from stents into readable formats for health professionals. The data displayed here is test/dummy data.</p>\n      <a href=\"\">\n      <img src={stentpic}></img>\n      </a>\n      <p>Technologies: Android SDK, Python, MySQL</p>\n    </div>\n  )\n}\n \nexport default Stent;","/home/nosbis/Documents/personal-site/src/Gems.js",["77"],"import React from \"react\"\nimport './App.css'\nimport gemspic from './images/gemspic.png'\n\nfunction Gems(props) {\n  return (\n    <div class=\"project-select\">\n      <h1>Glasgow Gems</h1>\n      <p>Second Year Group Project: Django Web Application, social network sharing architecture around Glasgow. Deployed for grading (not longer hosted), testing with a set of real users.</p>\n      <a href=\"https://github.com/sibsones/glasgowgems\">\n      <img src={gemspic}></img>\n      </a>\n      <p>Technologies: Django, sqlite3<